<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="762.0" prefWidth="994.0" styleClass="dashboard-background" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.javafxdemo.controller.DashboardController">

   <AnchorPane layoutX="45.0" layoutY="100.0" prefHeight="766.0" prefWidth="994.0" style="-fx-background-color: #778899;">
      <children>
         <Button fx:id="playQuizButton" alignment="BASELINE_CENTER" layoutX="655.0" layoutY="682.0" onAction="#playQuizButtonClicked" prefHeight="42.0" prefWidth="159.0" styleClass="play-quiz-button" text="Play Quiz" />
         <Label fx:id="accountNameText" layoutX="375.0" layoutY="655.0" prefHeight="84.0" prefWidth="148.0" />

         <Label fx:id="nationality" layoutX="375.0" layoutY="655.0" prefHeight="84.0" prefWidth="148.0" />
      </children>
      <VBox layoutX="155.0" layoutY="74.0" opacity="0.45" prefHeight="517.0" prefWidth="680.0" style="-fx-background-color: #191970;" AnchorPane.topAnchor="74.0">
         <children>
            <Label alignment="CENTER" contentDisplay="TOP" prefHeight="183.0" prefWidth="680.0" style="-fx-alignment: centre;" text="       DashBoard     " textAlignment="RIGHT" textFill="#e4e2e2">
               <font>
                  <Font name="System Bold" size="35.0" />
               </font>
            </Label>
         </children>
      </VBox>
      <Button layoutX="421.0" layoutY="273.0" onAction="#LoadResults" prefHeight="44.0" prefWidth="152.0" style="-fx-background-color: #191970;" text="View Scoreboard" textFill="#e8e4e4">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
      <Button layoutX="421.0" layoutY="339.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="152.0" style="-fx-background-color: #191970;" text="Profile Detail" textFill="#f8f5f5">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </AnchorPane>

</VBox>
